{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [ "error", "ignorePackages", { "": "never" } ],
        "linebreak-style": ["error", "windows"],
        "no-console": 0
    }
}
