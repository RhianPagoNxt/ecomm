{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": ["off", {"devDependencies": false, "optionalDependencies": false, "peerDependencies": false}],
        "import/extensions": [ "error", "ignorePackages", { "": "never" } ],
        "linebreak-style": ["error", "windows"],
        "no-unused-vars": 0,
        "no-console": 0
    }
}
